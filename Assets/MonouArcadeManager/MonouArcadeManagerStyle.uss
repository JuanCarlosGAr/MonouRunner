    background-size: 200% 100%;
    margin-top: 10px;
    margin-right: 0;
    margin-bottom: 10px;
    margin-left: 0;
}

.row1 {
    background-size: 200% 100%;
    background-image: url("project://database/Assets/Sprites/AssetsJungleMission.png?fileID=-470989066068876683&guid=b4a1c8e0f69d54c498d5fdac224af5c6&type=3#AssetsJungleMission_11");
    margin-top: 10px;
